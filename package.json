{
  "name": "emunova.net",
  "private": true,
  "version": "3.1.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "serve -p 3000 --cors --compress --favicon tmp/build/favicon.ico tmp/build",
    "test": "grunt test",
    "build": "npm run build-jquery",
    "build-jquery": "npm run build-jquery-legacy && npm run build-jquery-modern",
    "build-jquery-legacy": "jquery-builder --v 1.10.2 -e deprecated,effects,wrap > src/assets/js/jquery-legacy.js",
    "build-jquery-modern": "jquery-builder --v 2.0.3 -e deprecated,effects,wrap,sizzle > src/assets/js/jquery.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/oncletom/emunova.net.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/oncletom/emunova.net/issues"
  },
  "dependencies": {
    "assemble": "^0.4.24",
    "assemble-yaml": "^0.2.1",
    "grunt": "^0.4.1",
    "grunt-concurrent": "^0.5.0",
    "grunt-contrib-copy": "^0.4.1",
    "grunt-contrib-uglify": "^0.2.4",
    "grunt-gh-pages": "^0.7.1",
    "grunt-responsive-images": "^0.1.1",
    "grunt-shell": "^0.4.0",
    "grunt-uncss": "^0.3.6"
  },
  "devDependencies": {
    "grunt-contrib-jshint": "^0.6.4",
    "grunt-contrib-less": "^0.11.4",
    "grunt-contrib-watch": "^0.5.3",
    "jquery-builder": "^0.5.0",
    "serve": "^1.4.0",
    "time-grunt": "^0.1.1"
  }
}
