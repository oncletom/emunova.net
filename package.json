{
  "name": "emunova.net",
  "private": true,
  "version": "3.2.0",
  "description": "",
  "scripts": {
    "index": "node ./bin/index-content.js",
    "test": "exit 0",
    "less": "lessc --include-path=static/less/lib:node_modules/bootstrap/less",
    "dump": "echo $npm_package_scripts_build_css",
    "build": "npm-run-all -p 'build:*'",
    "build:js": "browserify -e static/assets/js/main.js -o static/bundle.js",
    "build:css":
      "$npm_package_scripts_less static/less/core.less static/bundle.css",
    "watch": "npm-run-all -p 'watch:*'",
    "watch:js": "watchify -vd -e static/assets/js/main.js -o static/bundle.js",
    "watch:css": "nodemon -e less -w static/less -x 'npm run build:css'"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/oncletom/emunova.net.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/oncletom/emunova.net/issues"
  },
  "dependencies": {
    "algoliasearch": "^3.14.0",
    "babel-preset-env": "^1.6.1",
    "babelify": "^7.3.0",
    "bootstrap": "~3.1.0",
    "browserify": "^13.0.0",
    "fast-sha256": "^1.0.0",
    "hyperquest": "^1.3.0",
    "jquery": "^2.2.3",
    "less": "^2.7.3",
    "lodash": "^4.11.1",
    "npm-run-all": "^1.8.0",
    "sorttable": "github:oncletom/sorttable#develop",
    "uglifyify": "^3.0.1"
  },
  "devDependencies": {
    "nodemon": "^1.12.1",
    "watchify": "^3.7.0"
  },
  "browser": {
    "bootstrap/collapse": "./node_modules/bootstrap/js/collapse.js",
    "bootstrap/transition": "./node_modules/bootstrap/js/transition.js",
    "bootstrap/dropdown": "./node_modules/bootstrap/js/dropdown.js"
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": ["env"]
        }
      ],
      "uglifyify"
    ]
  }
}
