{
  "name": "emunova.net",
  "private": true,
  "version": "3.1.2",
  "description": "",
  "main": "index.js",
  "scripts": {
    "index": "node ./bin/index-content.js",
    "serve": "serve -p 3000 --cors --compress --favicon tmp/build/favicon.ico tmp/build",
    "start": "npm-run-all --parallel serve watch",
    "test": "grunt test",
    "build": "npm-run-all build:content build:ui build:bundle",
    "build:cache": "grunt precache",
    "build:content": "grunt build-content",
    "build:bundle": "browserify -e src/assets/js/main.js -o tmp/build/assets/v${npm_package_version}/js/core.min.js",
    "build:ui": "grunt build-ui",
    "build:images": "grunt build-images",
    "watch": "npm-run-all --parallel watch:*",
    "prewatch:bundle": "npm run build:ui",
    "watch:bundle": "watchify -vd -e src/assets/js/main.js -o tmp/build/assets/v${npm_package_version}/js/core.min.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/oncletom/emunova.net.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/oncletom/emunova.net/issues"
  },
  "dependencies": {
    "algoliasearch": "^3.13.1",
    "assemble": "^0.4.24",
    "assemble-yaml": "^0.2.1",
    "babel-preset-es2015": "^6.6.0",
    "babelify": "^7.3.0",
    "bootstrap": "~3.1.0",
    "fast-sha256": "^1.0.0",
    "grunt": "^0.4.1",
    "grunt-concurrent": "^0.5.0",
    "grunt-contrib-copy": "^0.4.1",
    "grunt-contrib-less": "^1.3.0",
    "grunt-responsive-images": "^0.1.1",
    "grunt-shell": "^0.4.0",
    "grunt-uncss": "^0.5.0",
    "holderjs": "^2.9.3",
    "hyperquest": "^1.3.0",
    "jquery": "^2.2.3",
    "jquery-builder": "^0.5.0",
    "lazyloadjs": "^3.2.2",
    "lodash": "^4.11.1",
    "loose-envify": "^1.1.0",
    "npm-run-all": "^1.8.0",
    "sorttable": "github:oncletom/sorttable#develop",
    "time-grunt": "^0.1.1",
    "uglifyify": "^3.0.1"
  },
  "devDependencies": {
    "browserify": "^13.0.0",
    "grunt-cli": "^1.2.0",
    "grunt-contrib-jshint": "^0.6.4",
    "grunt-contrib-watch": "^0.5.3",
    "serve": "^1.4.0",
    "watchify": "^3.7.0"
  },
  "browser": {
    "bootstrap/collapse": "./node_modules/bootstrap/js/collapse.js",
    "bootstrap/transition": "./node_modules/bootstrap/js/transition.js",
    "bootstrap/dropdown": "./node_modules/bootstrap/js/dropdown.js"
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "es2015"
          ]
        }
      ],
      "loose-envify",
      "uglifyify"
    ]
  }
}
