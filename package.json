{
  "name": "emunova.net",
  "version": "3.0.4",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "serve -p 3000 --compress --favicon tmp/build/favicon.ico tmp/build",
    "test": "grunt test",
    "build": "npm run build-jquery",
    "build-jquery": "npm run build-jquery-legacy && npm run build-jquery-modern",
    "build-jquery-legacy": "jquery-builder --v 1.10.2 -e ajax,deprecated,effects,wrap > src/assets/js/jquery-legacy.js",
    "build-jquery-modern": "jquery-builder --v 2.0.3 -e ajax,deprecated,effects,wrap,sizzle > src/assets/js/jquery.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/oncletom/emunova.net.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/oncletom/emunova.net/issues"
  },
  "dependencies": {
    "grunt": "~0.4.1",
    "assemble": "~0.4.24",
    "grunt-contrib-copy": "~0.4.1",
    "grunt-responsive-images": "~0.1.1",
    "grunt-contrib-uglify": "~0.2.4",
    "grunt-gh-pages": "~0.7.1",
    "grunt-shell": "~0.4.0"
  },
  "devDependencies": {
    "grunt-contrib-watch": "~0.5.3",
    "grunt-contrib-jshint": "~0.6.4",
    "serve": "~1.3.0",
    "time-grunt": "~0.1.1",
    "grunt-contrib-less": "~0.7.0",
    "jquery-builder": "~0.5.0"
  }
}
