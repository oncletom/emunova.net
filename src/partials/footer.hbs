<script>
(function(d, w){
  document.body.addEventListener('click', function(e){
    var el = e.target;

    if (el.nodeName === "A" && el.href.match('github.io') && !el.href.match('github.io/emunova.net')){
      e.preventDefault();
      w.location.href = el.href.replace('github.io', 'github.io/emunova.net');
    }
  });
})(document, window);
</script>
